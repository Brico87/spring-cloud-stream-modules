        <fo:inline font-weight="bold" color="#7F0055">{</fo:inline>
            <fo:inline font-weight="bold" font-style="italic" color="#2A00FF">"category"</fo:inline>: <fo:inline font-weight="bold" font-style="italic" color="#2A00FF">"reference"</fo:inline><fo:inline font-weight="bold" color="#7F0055">,</fo:inline>
            <fo:inline font-weight="bold" font-style="italic" color="#2A00FF">"author"</fo:inline>: <fo:inline font-weight="bold" font-style="italic" color="#2A00FF">"Nigel Rees"</fo:inline><fo:inline font-weight="bold" color="#7F0055">,</fo:inline>
            <fo:inline font-weight="bold" font-style="italic" color="#2A00FF">"title"</fo:inline>: <fo:inline font-weight="bold" font-style="italic" color="#2A00FF">"Sayings of the Century"</fo:inline><fo:inline font-weight="bold" color="#7F0055">,</fo:inline>
            <fo:inline font-weight="bold" font-style="italic" color="#2A00FF">"price"</fo:inline>: 8.95
